http {
    include       mime.types;
    default_type  application/octet-stream;

    sendfile        on;

    keepalive_timeout  65;

upstream samplecluster{
  server localhost:9193 weight=10;
  server localhost:9191 weight=20;

}

server {
        listen       1111;
        server_name  jambo2;

               location / {
            proxy_pass   http://samplecluster;
            index  index.html index.htm;
        }

    }



